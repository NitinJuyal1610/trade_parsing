{
  "bin": "./bin/script.js",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts | npx pino-pretty",
    "start": "node dist/src/server.js | npx pino-pretty",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "debug-test": "jest --runInBand --detectOpenHandles",
    "debug-test-coverage": "jest --runInBand --detectOpenHandles --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "tsc -p .",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier:fix": "prettier --write src/**/*.ts",
    "prepare": "husky"
  },
  "keywords": [
    "typescript",
    "express",
    "starter",
    "boilerplate",
    "mongoose",
    "mongodb",
    "atlas",
    "pino",
    "jest",
    "eslint",
    "prettier",
    "husky",
    "nodemon",
    "supertest"
  ],
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "mongoose": "^8.4.0",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "onchange": "^7.1.0",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
